// #B06DDA African Violet
// #90CEE0 Sky Blue
// #E67B5B Burnt Sienna

{
	"name": "limbo",
	"type": "dark",
	"colors": {
        "activityBar.activeBackground": "#5b0096",
        "activityBar.activeFocusBorder": "#5b0096",
        "activityBar.background": "#1b1b1b",
        "activityBar.foreground": "#aaa",
		"activityBarBadge.background": "#5b0096",
		"editor.background": "#1b1b1b",
        "editor.foreground": "#ccc",
        "editor.findMatchBorder": "#fff",
        "editor.lineHighlightBackground": "#222",
        "editor.selectionBackground": "#333",
        "editor.selectionHighlightBackground": "#333",
        "focusBorder": "#1b1b1b",
        "list.hoverForeground": "#fff",
        "list.hoverBackground": "#1b1b1b",
        "list.activeSelectionBackground": "#333",
        "sideBar.background": "#1b1b1b",
        "sideBarSectionHeader.background": "#282828",
        "sideBarTitle.foreground": "#bbb"
	},
	"tokenColors": [
        {
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#666"
			}
		},
		{
			"name": "Keyword, Operator",
			"scope": [
				"constant.other.color",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
                "keyword.other.substitution",
                "keyword.operator",
                "entity.other.attribute-name"
			],
			"settings": {
                "foreground": "#aaa"
			}
        },
        {
			"name": "Keyword, Operator italic",
			"scope": [
                "constant.language",
				"storage.type",
                "storage.modifier",
                "keyword.control.as",
                "keyword.control.conditional",
                "keyword.control.default",
                "keyword.control.export",
                "keyword.control.flow",
                "keyword.control.from",
                "keyword.control.import",
                "keyword.function",
                "keyword.operator.expression",
                "meta.separator"
			],
			"settings": {
                "foreground": "#aaa",
                "fontStyle": "italic"
			}
        },
        {
			"name": "usual",
			"scope": [
                "meta.object",
                "meta.jsx.children",
                "support.type.property-name",
                "variable.object.property"
			],
			"settings": {
                "foreground": "#ccc"
			}
        },
        {
			"name": "Block Level Variables",
			"scope": [
                "meta.block",
                "entity.name",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
                "support.type.sys-types",
                "entity.other.inherited-class",
                "variable.other.readwrite.alias",
                "meta.block",
                "meta.definition",
                "constant.language.import-export-all",
                "support.type.exception",
                "meta.parameters",
                "entity.name.type.interface",
                "entity.name.type.class"
			],
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"name": "Block Level Variables italic",
			"scope": [
                "markup.italic",
                "variable.language.this"
			],
			"settings": {
                "foreground": "#fff",
                "fontStyle": "italic"
			}
		},
		{
			"name": "bold",
			"scope": [
                "markup.bold"
			],
			"settings": {
                "foreground": "#fff",
                "fontStyle": "bold"
			}
		},
        {
			"name": "African Violet",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
                "keyword.other.special-method",
                "support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
                "keyword.other",
                "meta.tag"
			],
			"settings": {
				"foreground": "#b078f5"
			}
		},
		{
			"name": "African Violet italic",
			"scope": [
                "support.type.primitive",

                "markup.quote",
                "punctuation.decorator"
			],
			"settings": {
                "foreground": "#b078f5",
                "fontStyle": "italic"

			}
        },
        {
			"name": "Burnt Sienna",
			"scope": [
                "constant.numeric",
                "punctuation.definition.list",
                "constant.character.entity",
                "meta.image.inline",
                "source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
                "foreground": "#E67B5B"
			}
        },
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
                "string",
				"constant.other.key",
				"markup.heading",
				"markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
                "foreground": "#78e6f5"

			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#b078f5"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#78e6f5"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#78e6f5"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
	]
}
